{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "custom-react-scripts": "0.0.23"
  },
  "dependencies": {
    "firebase": "^3.9.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "babel",
      "stage-0"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "components": "./components",
            "containers": "./containers",
            "style": "./style"
          }
        }
      ]
    ]
  }
}
